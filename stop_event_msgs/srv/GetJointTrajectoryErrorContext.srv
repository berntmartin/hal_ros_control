int32 INVALID_JOINTS = -2
int32 OLD_HEADER_TIMESTAMP = -3
int32 PATH_TOLERANCE_VIOLATED = -4
int32 GOAL_TOLERANCE_VIOLATED = -5
int32 PROBE_UNEXPECTED_RISING_EDGE = -6
int32 PROBE_UNEXPECTED_FALLING_EDGE = -7
int32 PROBE_CONTACT_AT_START = -8
int32 PROBE_INVALID_STATE = -9
int32 PROBE_REACHED_MOTION_END = -10
int32 HARDWARE_STOP_EVENT = -11
int32 HARDWARE_ESTOP_EVENT = -12
